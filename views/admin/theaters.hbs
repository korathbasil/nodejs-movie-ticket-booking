          <th class="admin-user-table-column2">Theater Name</th>
          <th class="admin-user-table-column3">Owner</th>
          <th class="admin-user-table-column4">Added Date</th>
        </tr>
        <tr>
          <td>1</td>
          <td>
            {{theaters.theater}}
          </td>
          <td>{{theaters.name}}</td>
          <td>22/10/2020</td>
        </tr>
      </table>
    </div>
  </section>
</div>

