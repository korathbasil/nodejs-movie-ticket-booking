  setActiveTab("theaters-link");
  {{! const rowNumbers = () =>
  { const columns
  =document.querySelectorAll(".admin-theater-owner-theaters-table-no")
  columns.forEach(column => { console.log(column.innerHTML) const value =
  parseInt(column.innerHTML); column.innerHTML = value + 1; }) } }}
